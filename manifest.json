{
  "manifest_version": 2,

  "name": "CatChat",
  "description": "Dank memes",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "all_frames": true,

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "<all_urls>"
  ]
}
